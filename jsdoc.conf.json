{
  "source": {
    "include": [
      "./src/",
      "./README.md"
    ],
    "includePattern": "\\.(js|ts)$",
    "exclude": [
      "node_modules/",
      "dist/",
      "tests/",
      "coverage/",
      "benchmarks/"
    ]
  },
  "opts": {
    "destination": "./docs/generated/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/typescript"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "hardwrap": true,
    "idInHeadings": true
  }
}